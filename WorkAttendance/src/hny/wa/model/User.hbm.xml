<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="wa.hny.model">
	<class name="hny.wa.model.User" table="user">
		<id column="id" name="id" type="integer">
			<generator class="increment" />
		</id>
		<property column="userid" name="userid" type="string" />
		<property column="username" name="username" type="string" />
		<property column="departid" name="departid" type="int" />
		<property column="power" name="power" type="int" />
	</class>
</hibernate-mapping>
