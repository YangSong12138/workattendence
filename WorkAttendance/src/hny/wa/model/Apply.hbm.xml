<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="wa.hny.model">
	<class name="hny.wa.model.Apply" table="Apply">
		<id column="id" name="id" type="integer">
			<generator class="increment" />
		</id>
		<property column="userid" name="userid" type="string" />
		<property column="applytype" name="applytype" type="int" />
		<property column="content" name="content" type="string" />
		<property column="starttime" name="starttime" type="string" />
		<property column="endtime" name="endtime" type="string" />
		<property column="applytime" name="applytime" type="string" />
		<property column="state" name="state" type="int" />
		<property column="reason" name="reason" type="string" />
	</class>

</hibernate-mapping>
